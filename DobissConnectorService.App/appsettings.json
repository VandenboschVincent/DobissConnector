{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "dobiss": {
    "Delay": 5000, // Delay in milliseconds to sync status of lights, set to 0 to disable
    "Modules": { // List of modules in the Dobiss system (RELAY, DIMMER, ZERO_TO_TEN_VOLT)
      "1": "RELAY", // Key of the module is important! (1, 2, 3, etc.)
      "2": "DIMMER",
      "3": "RELAY"
    },
    "DobissIp": "192.168.1.153", // IP address of the Dobiss system
    "DobissPort": 10001 // Port of the Dobiss system (default is 10001)
  },
  "mqtt": { // MQTT settings, can be left out to only use restapi
    "Ip": "192.168.1.34",
    "Port": 1883,
    "User": "", // User for the MQTT broker, leave empty if anonymous connection
    "Password": ""
  }
}
